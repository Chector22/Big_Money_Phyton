aws_items = []
aws_items.append('S3')
aws_items.append('Lambda')
aws_items.append('EC2') 
aws_items.append('DynamoDB') 
aws_items.append(CloudWatch') 
aws_items.append('IAM') 

print(aws_items) 

print(len(aws_items))

del aws_items[1]
del aws_items[2] 

print(aws_items)
print(len(aws_items))
